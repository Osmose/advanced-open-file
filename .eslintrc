{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "strict": 0,
        "quotes": [1, "single"],
        "no-alert": 0,
        "comma-dangle": 0,
        "camelcase": 0,
        "no-use-before-define": 0,
        "handle-callback-err": 0,
        "no-underscore-dangle": 0
    }
}
